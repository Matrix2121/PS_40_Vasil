[32m+[m[32m                Console.WriteLine("Enter name: ");[m
[32m+[m[32m                loginName = Console.ReadLine();[m
[32m+[m
[32m+[m[32m                if (loginName.Equals("exit"))[m
[32m+[m[32m                {[m
[32m+[m[32m                    break;[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                Console.WriteLine("Enter password: ");[m
[32m+[m[32m                loginPass = Console.ReadLine();[m
[32m+[m
[32m+[m[32m                if (loginPass.Equals("exit"))[m
[32m+[m[32m                {[m
[32m+[m[32m                    break;[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                var user = (from user1 in users[m
[32m+[m[32m                           where user1.Names == loginName && user1.Password == loginPass[m
[32m+[m[32m                           select user1).FirstOrDefault();[m
[32m+[m
[32m+[m[32m                if (user != null)[m
[32m+[m[32m                {[m
[32m+[m[32m                    Console.WriteLine("User exists");[m
[32m+[m[32m                }[m
[32m+[m[32m                else[m
[32m+[m[32m                {[m
[32m+[m[32m                    Console.WriteLine("User doesn't exist");[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                if (loginName.Equals("exit") || loginPass.Equals("exit"))[m
[32m+[m[32m                {[m
[32m+[m[32m                    break;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
