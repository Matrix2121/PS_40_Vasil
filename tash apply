[32m+[m[32m            context.Database.EnsureCreated();[m
[32m+[m[32m            context.Add<DatabaseUser>(new DatabaseUser()[m
[32m+[m[32m            {[m
[32m+[m[32m                Names = "user",[m
[32m+[m[32m                Password = "passsword",[m
[32m+[m[32m                Expires = DateTime.Now,[m
[32m+[m[32m                Role = UserRolesEnum.STUDENT[m
[32m+[m[32m            });[m
[32m+[m[32m            context.SaveChanges();[m
[32m+[m[32m            var users = context.Users.ToList();[m
[32m+[m
[32m+[m[32m            User login = new User();[m
[32m+[m[32m            string loginName;[m
[32m+[m[32m            string loginPass;[m
[32m+[m
